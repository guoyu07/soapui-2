{
  if (params != null) {
    StringBuilder sb=new StringBuilder();
    Iterator it=params.entrySet().iterator();
    while (it.hasNext()) {
      Map.Entry pairs=(Map.Entry)it.next();
      sb.append(String.format("%s%s: %s",(sb.toString().equals("") ? "" : ", "),pairs.getKey(),pairs.getValue()));
    }
    return sb.toString();
  }
 else {
    return "";
  }
}
