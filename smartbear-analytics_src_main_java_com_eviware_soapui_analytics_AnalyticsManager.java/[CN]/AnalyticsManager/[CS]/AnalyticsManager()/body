{
  Date date=new Date();
  String newString=new SimpleDateFormat("yyyy-MM-dd HH:mm:ss").format(date);
  try {
    sessionId=String.format("AutoGeneratedSessionTimeStamp:%s:UserId:%s",newString,ActionDescription.getUserId());
  }
 catch (  Exception e) {
    sessionId=String.format("AutoGeneratedSessionTimeStamp:%s",newString);
  }
