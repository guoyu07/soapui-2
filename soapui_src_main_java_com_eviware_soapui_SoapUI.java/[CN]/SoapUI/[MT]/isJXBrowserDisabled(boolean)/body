{
  if (UISupport.isHeadless() || isCommandLine())   return true;
  String disable=System.getProperty("soapui.jxbrowser.disable","nope");
  if (disable.equals("true"))   return true;
  if (getSoapUICore() != null && getSettings().getBoolean(UISettings.DISABLE_BROWSER))   return true;
  if (!disable.equals("false") && allowNative && (BrowserType.Mozilla.isSupported() || BrowserType.IE.isSupported() || BrowserType.Safari.isSupported()))   return false;
  return !disable.equals("false") && (!PlatformContext.isMacOS() && "64".equals(System.getProperty("sun.arch.data.model")));
}
