{
  String fn=fileName;
  if (!new File(fileName).exists()) {
    try {
      fileName=importSettingsOnStartup(fileName);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
  Settings result=super.initSettings(fileName);
  if (!fileName.equals(fn))   setSettingsFile(fn);
  return result;
}
