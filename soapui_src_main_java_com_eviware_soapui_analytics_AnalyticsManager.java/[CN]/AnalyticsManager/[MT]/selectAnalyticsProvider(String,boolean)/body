{
  for (int i=0; i < factories.size(); i++) {
    AnalyticsProviderFactory factory=factories.get(i);
    if (factory.getName().compareToIgnoreCase(name) == 0) {
      registerActiveProvider(factory.allocateProvider(),keepTheOnlyOne);
      return true;
    }
  }
  if (keepTheOnlyOne) {
    providers.clear();
  }
  return false;
}
