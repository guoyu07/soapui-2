{
  if (StringUtils.hasContent(newValue)) {
    int ix=newValue.lastIndexOf('.');
    if (ix > 0)     newValue=newValue.substring(0,ix);
    ix=newValue.lastIndexOf('/');
    if (ix == -1)     ix=newValue.lastIndexOf('\\');
    if (ix != -1)     newValue=newValue.substring(ix + 1);
    return newValue;
  }
  return DEFAULT_PROJECT_NAME;
}
