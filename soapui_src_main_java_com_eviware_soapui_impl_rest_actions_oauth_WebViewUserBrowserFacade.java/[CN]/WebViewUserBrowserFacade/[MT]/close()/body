{
  try {
    SwingUtilities.invokeLater(new Runnable(){
      @Override public void run(){
        popupWindow.setVisible(false);
        popupWindow.dispose();
      }
    }
);
  }
 catch (  Exception e) {
    SoapUI.log.debug("Could not close window due to unexpected error: " + e.getMessage() + "!");
  }
}
