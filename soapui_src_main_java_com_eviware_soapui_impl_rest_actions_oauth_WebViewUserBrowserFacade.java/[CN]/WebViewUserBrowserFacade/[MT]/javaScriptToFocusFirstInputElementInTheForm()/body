  return "var bFound = false;\n" + "\n" + "  for (f=0; f < document.forms.length; f++)\n"+ "  {\n"+ "    for(i=0; i < document.forms[f].length; i++)\n"+ "    {\n"+ "      if (document.forms[f][i].type != \"hidden\" && document.forms[f][i].disabled != true)\n"+ "      {"+ "        document.forms[f][i].focus();\n"+ "        var bFound = true;\n"+ "      }\n"+ "      if (bFound == true)\n"+ "        break;\n"+ "    }\n"+ "    if (bFound == true)\n"+ "      break;\n"+ "  }";
