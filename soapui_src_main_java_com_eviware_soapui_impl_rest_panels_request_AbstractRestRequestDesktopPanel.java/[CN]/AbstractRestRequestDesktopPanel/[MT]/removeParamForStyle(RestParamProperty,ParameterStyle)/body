{
switch (style) {
case QUERY:
    resetQueryPanelText();
  break;
case TEMPLATE:
setResourcePanelText(getResourcePanelText().replaceAll("\\{" + property.getName() + "\\}",""));
break;
case MATRIX:
String propValueAtRequestLevel=getRequest().getParams().getProperty(property.getName()).getValue();
setResourcePanelText(getResourcePanelText().replaceAll(";" + property.getName() + "="+ propValueAtRequestLevel,""));
break;
default :
break;
}
}
