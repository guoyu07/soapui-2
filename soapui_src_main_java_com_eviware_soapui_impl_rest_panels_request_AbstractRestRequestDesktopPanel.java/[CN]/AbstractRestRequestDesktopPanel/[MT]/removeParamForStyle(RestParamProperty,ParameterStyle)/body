{
switch (style) {
case QUERY:
    resetQueryPanelText();
  break;
case TEMPLATE:
resourcePanel.setText(resourcePanel.getText().replaceAll("\\{" + property.getName() + "\\}",""));
break;
case MATRIX:
resourcePanel.setText(resourcePanel.getText().replaceAll(";" + property.getName() + "="+ property.getValue(),""));
break;
default :
break;
}
}
