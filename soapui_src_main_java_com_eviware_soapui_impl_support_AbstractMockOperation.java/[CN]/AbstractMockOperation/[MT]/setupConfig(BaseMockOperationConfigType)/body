{
  Operation operation=getOperation();
  if (!config.isSetName())   config.setName(operation == null ? "<missing operation>" : operation.getName());
  if (!config.isSetDefaultResponse() && getMockResponseCount() > 0)   setDefaultResponse(getMockResponseAt(0).getName());
  if (!config.isSetDispatchStyle()) {
    config.setDispatchStyle(MockOperationDispatchStyleConfig.SEQUENCE);
    setExampleScript();
  }
  dispatcher=MockOperationDispatchRegistry.buildDispatcher(getConfig().getDispatchStyle().toString(),this);
}
