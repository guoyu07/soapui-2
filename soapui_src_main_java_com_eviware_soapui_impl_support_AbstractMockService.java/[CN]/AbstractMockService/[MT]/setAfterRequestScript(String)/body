{
  String oldScript=getAfterRequestScript();
  if (!getConfig().isSetAfterRequestScript())   getConfig().addNewAfterRequestScript();
  getConfig().getAfterRequestScript().setStringValue(script);
  if (afterRequestScriptEnginePool != null)   afterRequestScriptEnginePool.setScript(script);
  notifyPropertyChanged("afterRequestScript",oldScript,script);
}
