{
  if (anonymous.equals(AnonymousTypeConfig.REQUIRED.toString()))   getConfig().setAnonymous(AnonymousTypeConfig.REQUIRED);
 else   if (anonymous.equals(AnonymousTypeConfig.PROHIBITED.toString()))   getConfig().setAnonymous(AnonymousTypeConfig.PROHIBITED);
 else   getConfig().setAnonymous(AnonymousTypeConfig.OPTIONAL);
}
