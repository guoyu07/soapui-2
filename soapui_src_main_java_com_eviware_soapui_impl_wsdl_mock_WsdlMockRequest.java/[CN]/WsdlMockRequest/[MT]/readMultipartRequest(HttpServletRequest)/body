{
  StringToStringMap values=StringToStringMap.fromHttpHeader(request.getContentType());
  mockRequestDataSource=new MockRequestDataSource(request);
  mmSupport=new MultipartMessageSupport(mockRequestDataSource,values.get("start"),null,true,requestContext.getMockService().getSettings().getBoolean(WsdlSettings.PRETTY_PRINT_RESPONSE_MESSAGES));
}
