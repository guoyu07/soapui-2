{
  try {
    if (inSocket != null) {
      inSocket.close();
    }
    if (outSocket != null) {
      outSocket.close();
    }
    inSocket=null;
    outSocket=null;
    if (in != null) {
      in.close();
    }
    if (out != null) {
      out.close();
    }
    in=null;
    out=null;
    done=true;
  }
 catch (  Exception e) {
    SoapUI.log.info("Error halting socket: " + e.toString());
  }
}
