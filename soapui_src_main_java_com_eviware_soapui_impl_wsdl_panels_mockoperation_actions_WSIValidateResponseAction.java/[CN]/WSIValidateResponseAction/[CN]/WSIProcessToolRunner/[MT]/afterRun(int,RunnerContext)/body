{
  try {
    if (exitCode == 0 && context.getStatus() == RunnerContext.RunnerStatus.FINISHED) {
      WSIReportPanel panel=new WSIReportPanel(reportFile,configFile,logFile,true);
      panel.setPreferredSize(new Dimension(600,400));
      UISupport.showDesktopPanel(new DefaultDesktopPanel("WS-I Report","WS-I Report for validation of messages in MockResponse [" + modelItem.getName() + "]",panel));
    }
  }
 catch (  Exception e) {
    UISupport.showErrorMessage(e);
  }
}
