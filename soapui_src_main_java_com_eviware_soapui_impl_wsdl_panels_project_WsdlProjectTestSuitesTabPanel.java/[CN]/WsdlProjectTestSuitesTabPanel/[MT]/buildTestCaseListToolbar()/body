{
  JXToolBar toolbar=UISupport.createToolbar();
  toolbar.add(UISupport.createToolbarButton(SwingActionDelegate.createDelegate(AddNewTestSuiteAction.SOAPUI_ACTION_ID,project,null,"/testSuite.gif")));
