{
synchronized (this) {
    int ix=items.indexOf(assertion);
    if (ix == -1)     return;
    assertion.removePropertyChangeListener(this);
    items.remove(ix);
    fireIntervalRemoved(this,ix,ix);
    while (ix < items.size() && items.get(ix) instanceof AssertionError) {
      items.remove(ix);
      fireIntervalRemoved(this,ix,ix);
    }
  }
}
