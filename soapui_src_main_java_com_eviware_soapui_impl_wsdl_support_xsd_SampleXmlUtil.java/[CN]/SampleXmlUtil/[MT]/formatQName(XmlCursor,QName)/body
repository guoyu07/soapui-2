{
  XmlCursor parent=xmlc.newCursor();
  parent.toParent();
  String prefix=parent.prefixForNamespace(qName.getNamespaceURI());
  parent.dispose();
  String name;
  if (prefix == null || prefix.length() == 0)   name=qName.getLocalPart();
 else   name=prefix + ":" + qName.getLocalPart();
  return name;
}
