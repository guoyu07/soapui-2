{
  SoapUIClassLoaderState state=SoapUIExtensionClassLoader.ensure();
  try {
    log.info("Loading schema types from [" + wsdlUrl + "]");
    ArrayList<XmlObject> schemas=new ArrayList<XmlObject>(getSchemas(wsdlUrl,loader).values());
    return buildSchemaTypes(schemas);
  }
 catch (  Exception e) {
    SoapUI.logError(e);
    if (e instanceof SchemaException)     throw (SchemaException)e;
 else     throw new SchemaException("Error loading schema types",e);
  }
 finally {
    state.restore();
  }
}
