{
  testStep.beforeSave();
  TestStepConfig newStepConfig=(TestStepConfig)testStep.getConfig().copy();
  newStepConfig.setName(name);
  WsdlTestStep result=insertTestStep(newStepConfig,index,createCopy);
  if (result == null) {
    return null;
  }
  if (createCopy) {
    ModelSupport.createNewIds(result);
  }
  resolveTestCase();
  return result;
}
