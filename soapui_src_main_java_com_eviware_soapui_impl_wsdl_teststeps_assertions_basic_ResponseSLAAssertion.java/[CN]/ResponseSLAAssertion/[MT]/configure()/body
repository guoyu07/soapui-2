{
  String value=SLA;
  if (value == null || value.trim().length() == 0) {
    value="200";
  }
  value=UISupport.prompt("Specify desired response time","Configure Response SLA Assertion",value);
  try {
    Long.parseLong(value);
    SLA=value;
  }
 catch (  Exception e) {
    return false;
  }
  setConfiguration(createConfiguration());
  return true;
}
