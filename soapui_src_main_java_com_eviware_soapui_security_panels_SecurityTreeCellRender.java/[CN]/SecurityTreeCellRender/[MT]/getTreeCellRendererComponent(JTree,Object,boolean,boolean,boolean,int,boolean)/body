{
  Component result=null;
  if (released) {
    if (node instanceof TestStepNode)     result=getTreeCellRendererTestNode(arg0,(TestStepNode)node,sel,exp,leaf,arg5,arg6);
    if (node instanceof SecurityScanNode)     result=getTreeCellRendererSecurityScanNode(arg0,(SecurityScanNode)node,sel,exp,leaf,arg5,arg6);
    return result;
  }
  this.tree=arg0;
  if (componentTree.containsKey(node)) {
    result=componentTree.get(node);
    ((CustomTreeNode)result).setExpandedIcon(exp);
    ((CustomTreeNode)result).updateLabel();
    ((CustomTreeNode)result).setSelected(sel);
  }
 else {
    if (node instanceof TestStepNode)     result=getTreeCellRendererTestNode(arg0,(TestStepNode)node,sel,exp,leaf,arg5,arg6);
    if (node instanceof SecurityScanNode)     result=getTreeCellRendererSecurityScanNode(arg0,(SecurityScanNode)node,sel,exp,leaf,arg5,arg6);
    componentTree.put((DefaultMutableTreeNode)node,result);
  }
  return result;
}
