{
  ArrayList<SecurityCheckedParameter> parameterToRemove=new ArrayList<SecurityCheckedParameter>();
  for (  SecurityCheckedParameter param : params)   if (param.getName().equals(oldName)) {
    parameterToRemove.add(param);
  }
  for (  SecurityCheckedParameter param : parameterToRemove)   ((SecurityCheckedParameterImpl)param).setName(newName);
}
