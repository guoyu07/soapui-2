{
  try {
    String documentContent=fetchVersionDocumentContent(new URL(LATEST_VERSION_XML_LOCATION));
    getLatestVersionAvailable(documentContent);
  }
 catch (  Exception e) {
    SoapUI.log(e.getMessage());
    return;
  }
  if (isNewReleaseAvailable() && (!skipThisVersion() || helpAction))   showNewVersionDownloadDialog();
 else   if (helpAction)   UISupport.showInfoMessage("You are running the latest version of SoapUI!","Version Check");
}
