{
  if (modelItem == null) {
    return null;
  }
  try {
    UISupport.setHourglassCursor();
    SoapUIDesktop desktop=SoapUI.getDesktop();
    Analytics.tackActiveScreen(modelItem.getName());
    return desktop == null ? null : desktop.showDesktopPanel(modelItem);
  }
  finally {
    UISupport.resetCursor();
  }
}
