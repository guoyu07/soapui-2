{
  if (SoapUI.isJXBrowserDisabled())   return;
  if (errorPage != null)   setErrorPage(errorPage);
  this.url=url;
  if (browser == null) {
    if (!initBrowser())     return;
  }
  configureBrowser();
  updateJXBrowserProxy();
  if (postData != null && postData.length() > 0) {
    browser.navigate(url,postData);
  }
 else {
    browser.navigate(url);
  }
  if (showingErrorPage)   showingErrorPage=false;
}
