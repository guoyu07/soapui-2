{
  if (SoapUI.isJXBrowserDisabled())   return;
  if (browser == null) {
    if (!initBrowser())     return;
  }
  configureBrowser();
  try {
    browser.setContent(contentAsString,contextUri);
    pcs.firePropertyChange("content",null,null);
  }
 catch (  Throwable e) {
    e.printStackTrace();
  }
}
