{
  if (columnName.equals("Name")) {
    return 0;
  }
 else   if (columnName.equals("Content type")) {
    return 1;
  }
 else   if (columnName.equals("Size")) {
    return 2;
  }
 else   if (columnName.equals("Part")) {
    return 3;
  }
 else   if (columnName.equals("Type")) {
    return 4;
  }
  return -1;
}
