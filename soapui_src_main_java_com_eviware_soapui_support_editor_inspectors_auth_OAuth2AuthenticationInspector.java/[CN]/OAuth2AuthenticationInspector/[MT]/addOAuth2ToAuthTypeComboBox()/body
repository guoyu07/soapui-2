{
  String[] options={CredentialsConfig.AuthType.GLOBAL_HTTP_SETTINGS.toString(),CredentialsConfig.AuthType.PREEMPTIVE.toString(),CredentialsConfig.AuthType.NTLM.toString(),CredentialsConfig.AuthType.SPNEGO_KERBEROS.toString(),CredentialsConfig.AuthType.O_AUTH_2.toString()};
  JComboBox comboBox=(JComboBox)getAuthTypeForm().getComponent(COMBO_BOX_LABEL);
  getAuthTypeForm().setComboBoxItems(AUTH_TYPE_PROPERTY_NAME,comboBox,options);
}
