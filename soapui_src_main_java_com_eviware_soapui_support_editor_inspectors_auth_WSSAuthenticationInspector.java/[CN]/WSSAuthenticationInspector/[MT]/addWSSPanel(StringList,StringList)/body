{
  SimpleBindingForm basicForm=getBasicForm();
  basicForm.addSpace(GROUP_SPACING);
  basicForm.appendComboBox("outgoingWss","Outgoing WSS",outgoingNames.toStringArray(),"The outgoing WS-Security configuration to use");
