{
synchronized (this) {
    if (scriptEngines.isEmpty()) {
      if (modelItem != null)       scriptEngines.push(SoapUIScriptEngineRegistry.create(modelItem));
 else       scriptEngines.push(SoapUIScriptEngineRegistry.getFactory(id).createScriptEngine(null));
    }
    SoapUIScriptEngine result=scriptEngines.pop();
    if (script != null)     result.setScript(script);
    borrowed++;
    return result;
  }
}
