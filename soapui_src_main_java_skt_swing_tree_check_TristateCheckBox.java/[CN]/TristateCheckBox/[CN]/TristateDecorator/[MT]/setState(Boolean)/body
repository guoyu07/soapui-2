{
  if (state == Boolean.FALSE) {
    other.setArmed(false);
    setPressed(false);
    setSelected(false);
  }
 else   if (state == Boolean.TRUE) {
    other.setArmed(false);
    setPressed(false);
    setSelected(true);
  }
 else {
    other.setArmed(true);
    setPressed(true);
    setSelected(true);
  }
}
