{
  String jsonBody="{\n" + "  \"access_token\" : \"" + DUMMY_ACCESS_TOKEN + "\",\n"+ "  \"token_type\" : \"Bearer\",\n"+ "  \"expires_in\" : "+ EXPIRES_IN+ ",\n"+ "  \"refresh_token\" : \""+ DUMMY_REFRESH_TOKEN+ "\"\n"+ "}";
  tokenResponse.setBody(jsonBody);
  assertTokenResponseParameters(tokenResponse);
}
