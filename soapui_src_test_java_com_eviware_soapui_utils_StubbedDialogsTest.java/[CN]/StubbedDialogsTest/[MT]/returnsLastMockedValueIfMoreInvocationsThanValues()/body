{
  dialogs.mockConfirmWithReturnValue(true,false);
  assertThat(UISupport.confirmOrCancel("",""),equalTo(true));
  assertThat(UISupport.confirmOrCancel("",""),equalTo(false));
  assertThat(UISupport.confirmOrCancel("",""),equalTo(false));
  assertThat(UISupport.confirmOrCancel("",""),equalTo(false));
}
