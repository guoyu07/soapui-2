{
  for (  ModelItem target : targets) {
    if (target instanceof WsdlTestStep)     ((WsdlTestStep)target).setDisabled(true);
 else     if (target instanceof WsdlTestCase)     ((WsdlTestCase)target).setDisabled(true);
 else     if (target instanceof WsdlTestSuite)     ((WsdlTestSuite)target).setDisabled(true);
  }
}
