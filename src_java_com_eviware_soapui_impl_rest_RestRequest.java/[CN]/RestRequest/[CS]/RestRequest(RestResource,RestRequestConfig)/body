{
  super(requestConfig,resource,null,false);
  if (requestConfig.getParameters() == null)   requestConfig.addNewParameters();
  if (!requestConfig.isSetMethod())   setMethod(RequestMethod.GET);
  if (!requestConfig.isSetMediaType())   setMediaType(DEFAULT_MEDIATYPE);
  if (requestConfig.getParameters() == null)   requestConfig.addNewParameters();
  params=new XmlBeansRestParamsTestPropertyHolder(this,requestConfig.getParameters());
}
