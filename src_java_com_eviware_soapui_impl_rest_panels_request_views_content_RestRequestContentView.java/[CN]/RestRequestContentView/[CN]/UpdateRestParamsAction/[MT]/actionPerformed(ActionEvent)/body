{
  String str=UISupport.prompt("Enter new url below","Extract Params","");
  if (str == null)   return;
  try {
    restRequest.getParams().resetValues();
    RestUtils.extractParams(new URL(str),restRequest.getParams(),false);
    paramsTable.refresh();
  }
 catch (  Exception e1) {
    UISupport.showErrorMessage(e1);
  }
}
