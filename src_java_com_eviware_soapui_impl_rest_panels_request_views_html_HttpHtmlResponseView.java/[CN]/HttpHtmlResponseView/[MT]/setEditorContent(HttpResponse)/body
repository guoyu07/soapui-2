{
  if (httpResponse != null && httpResponse.getContentAsString() != null) {
    try {
      browser.setContent(new String(httpResponse.getContentAsString().getBytes("utf-8")),httpResponse.getURL().toURI().toString());
      hasResponseForRecording=true;
    }
 catch (    Throwable e) {
      e.printStackTrace();
    }
  }
 else {
    browser.setContent("<missing content>");
    hasResponseForRecording=false;
  }
}
