{
  Element elm=item.getElement();
  String type=elm.getAttribute(attribute);
  if (type.length() > 0) {
    int ix=type.indexOf(':');
    if (ix != -1)     type=type.substring(ix + 1);
    TreePath treePath=pathMap.get("/" + getModelItem().getName() + "/"+ targetGroup+ "/"+ type);
    if (treePath != null) {
      tree.setSelectionPath(treePath);
    }
  }
}
