{
  String content=response.getContentAsString();
  if (XmlUtils.seemsToBeXml(content))   return content;
  String result="<data contentType=\"" + response.getContentType() + "\" contentLength=\""+ response.getContentLength()+ "\">";
  for (int c=0; c < content.length(); c++) {
    if (content.charAt(c) < 8) {
      return result + new BASE64Encoder().encode(content.getBytes()) + "</data>";
    }
  }
  return result + "<![CDATA[" + content+ "]]></data>";
}
