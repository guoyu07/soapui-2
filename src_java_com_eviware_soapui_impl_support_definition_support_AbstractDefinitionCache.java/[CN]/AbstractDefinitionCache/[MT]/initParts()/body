{
  parts=new ArrayList<InterfaceDefinitionPart>();
  List<DefintionPartConfig> partList=definitionCache.getPartList();
  for (  DefintionPartConfig part : partList) {
    ConfigInterfaceDefinitionPart configInterfaceDefinitionPart=new ConfigInterfaceDefinitionPart(part,part.getUrl().equals(definitionCache.getRootPart()),definitionCache.getType());
    parts.add(configInterfaceDefinitionPart);
    if (configInterfaceDefinitionPart.isRootPart())     rootPart=configInterfaceDefinitionPart;
  }
}
