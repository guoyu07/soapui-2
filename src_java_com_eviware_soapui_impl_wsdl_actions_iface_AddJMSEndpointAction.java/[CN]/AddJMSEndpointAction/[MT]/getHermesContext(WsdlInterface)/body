{
  WsdlProject project=iface.getProject();
  try {
    Context ctx=HermesUtils.hermesContext(project);
    return ctx;
  }
 catch (  Exception e) {
    SoapUI.logError(e);
    UISupport.showErrorMessage("Project#Hermes Config property doesnt point to the proper hermes-config.xml folder");
  }
  return null;
}
