{
  if (getProgress() > 1 && loadTest.getCancelOnReachedLimit()) {
    testRunner.cancel("LoadTest Limit reached");
  }
 else   for (  LoadTestRunListener listener : loadTest.getLoadTestRunListeners()) {
    listener.beforeTestStep(WsdlLoadTestRunner.this,context,testRunner,runContext,runContext.getCurrentStep());
  }
}
