{
  try {
    if (rr1 != null) {
      rr1.halt();
    }
    if (rr2 != null) {
      rr2.halt();
    }
    if (inSocket != null) {
      inSocket.close();
    }
    inSocket=null;
    if (outSocket != null) {
      outSocket.close();
    }
    outSocket=null;
  }
 catch (  Exception e) {
    SoapUI.log.info("Error halting connection: " + e.toString());
  }
}
