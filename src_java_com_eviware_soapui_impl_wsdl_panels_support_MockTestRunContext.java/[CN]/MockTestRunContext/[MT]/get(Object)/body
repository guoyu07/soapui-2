{
  if ("currentStep".equals(key))   return getCurrentStep();
  if ("currentStepIndex".equals(key))   return getCurrentStepIndex();
  if ("settings".equals(key))   return getSettings();
  if ("testCase".equals(key))   return getTestCase();
  if ("testRunner".equals(key))   return getTestRunner();
  Object result=getProperty(key.toString());
  if (result == null) {
    result=super.get(key);
  }
  return result;
}
