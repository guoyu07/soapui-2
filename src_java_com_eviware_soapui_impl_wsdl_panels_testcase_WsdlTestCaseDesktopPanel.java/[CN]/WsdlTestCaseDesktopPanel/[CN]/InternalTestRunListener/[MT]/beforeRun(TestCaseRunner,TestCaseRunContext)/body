{
  if (SoapUI.getTestMonitor().hasRunningLoadTest(getModelItem()) || SoapUI.getTestMonitor().hasRunningSecurityTest(getModelItem()))   return;
  runButton.setEnabled(false);
  cancelButton.setEnabled(true);
  testStepList.setEnabled(false);
  testStepList.setSelectedIndex(-1);
  testCaseLog.clear();
  testCaseLog.addText("Test started at " + dateFormat.format(new Date()));
  WsdlTestCaseDesktopPanel.this.beforeRun();
  if (runner == null)   runner=testRunner;
}
