{
  try {
    for (int i=0; i < listeners.length; i++) {
      if (!listeners[i].beforeSubmit(this,submitContext)) {
        status=Status.CANCELED;
        System.err.println("listener cancelled submit..");
        return;
      }
    }
    status=Status.RUNNING;
    testStep.runQuery();
    if (status != Status.CANCELED) {
      status=Status.FINISHED;
    }
  }
 catch (  Exception e) {
    UISupport.showErrorMessage("There's been an error in executing query " + e.toString());
    error=e;
  }
 finally {
    if (status != Status.CANCELED) {
      for (int i=0; i < listeners.length; i++) {
        try {
          listeners[i].afterSubmit(this,submitContext);
        }
 catch (        Throwable e) {
          SoapUI.logError(e);
        }
      }
    }
  }
}
