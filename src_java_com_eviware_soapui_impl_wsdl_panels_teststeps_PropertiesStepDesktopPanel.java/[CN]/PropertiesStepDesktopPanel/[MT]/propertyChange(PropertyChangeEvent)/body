{
  if (!updatingSource && evt.getPropertyName().equals(WsdlPropertiesTestStep.SOURCE_PROPERTY))   sourceField.setText(evt.getNewValue().toString());
 else   if (!updatingTarget && evt.getPropertyName().equals(WsdlPropertiesTestStep.TARGET_PROPERTY))   targetField.setText(evt.getNewValue().toString());
  super.propertyChange(evt);
}
