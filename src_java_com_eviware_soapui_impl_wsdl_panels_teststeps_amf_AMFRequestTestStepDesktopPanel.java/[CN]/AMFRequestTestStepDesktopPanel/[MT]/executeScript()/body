{
  try {
    scriptEngine.setScript(groovyEditor.getEditArea().getText());
    scriptEngine.setVariable("log",log);
    Object result=scriptEngine.run();
    return result == null ? null : result.toString();
  }
 catch (  Throwable e) {
    SoapUI.logError(e);
  }
 finally {
    scriptEngine.clearVariables();
  }
  return null;
}
