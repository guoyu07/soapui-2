{
  try {
    String[] header=bodyPart.getHeader("Content-Id");
    if (header == null || header.length == 0)     return "<missing name>";
    if (header[0].startsWith("<") && header[0].endsWith(">"))     header[0]=header[0].substring(1,header[0].length() - 1);
    return header[0];
  }
 catch (  MessagingException e) {
    SoapUI.logError(e);
    return null;
  }
}
