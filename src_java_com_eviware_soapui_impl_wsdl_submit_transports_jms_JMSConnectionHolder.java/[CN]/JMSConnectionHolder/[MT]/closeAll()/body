{
  try {
    if (topicSession != null)     topicSession.close();
    if (queueSession != null)     queueSession.close();
    if (topicConnection != null)     topicConnection.close();
    if (queueConnection != null)     queueConnection.close();
  }
 catch (  JMSException e) {
    SoapUI.logError(e);
  }
}
