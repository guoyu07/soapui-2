{
  try {
    MessageExchange me=((MessageExchangeModelItem)messageExchange).getMessageExchange();
    if (me != null) {
      String r=me.getProperty("Endpoint");
      return r != null && r.startsWith(JMS_ENDPIONT_PREFIX);
    }
 else {
      return false;
    }
  }
 catch (  NullPointerException e) {
    SoapUI.logError(e);
  }
  return false;
}
