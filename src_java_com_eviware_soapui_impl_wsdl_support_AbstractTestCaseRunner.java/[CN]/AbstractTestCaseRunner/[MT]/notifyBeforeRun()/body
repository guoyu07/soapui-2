{
  if (testRunListeners == null || testRunListeners.length == 0)   return;
  for (int i=0; i < testRunListeners.length; i++) {
    try {
      if (Arrays.asList(getTestCase().getTestRunListeners()).contains(testRunListeners[i]))       testRunListeners[i].beforeRun(this,getRunContext());
    }
 catch (    Throwable t) {
      SoapUI.logError(t);
    }
  }
}
