{
  Header contentType=method.getResponseHeader("Content-Type");
  Header contentEncoding=method.getResponseHeader("Content-Encoding");
  String compressionAlg=contentType == null ? null : CompressionSupport.getAvailableAlgorithm(contentType.getValue());
  if (compressionAlg != null)   return compressionAlg;
  if (contentEncoding == null)   return null;
 else   return CompressionSupport.getAvailableAlgorithm(contentEncoding.getValue());
}
