{
  Policy innerPolicy=newAddressing.addNewPolicy();
  Element anonymousElm=XmlUtils.getFirstChildElementNS(addressingPolicy,new QName(WsaUtils.WSAM_NAMESPACE,"AnonymousResponses"));
  if (anonymousElm != null) {
    innerPolicy.addNewAnonymousResponses();
  }
 else {
    Element nonAnonymousElement=XmlUtils.getFirstChildElementNS(addressingPolicy,new QName(WsaUtils.WSAM_NAMESPACE,"NonAnonymousResponses"));
    if (nonAnonymousElement != null) {
      innerPolicy.addNewNonAnonymousResponses();
    }
  }
}
