{
  SoapUIClassLoaderState state=SoapUIExtensionClassLoader.ensure();
  try {
    soapSchemaXml=XmlObject.Factory.parse(SoapUI.class.getResource("/com/eviware/soapui/resources/xsds/soapEnvelope12.xsd"));
    soapSchema=XmlBeans.loadXsd(new XmlObject[]{soapSchemaXml});
    soapEncodingXml=XmlObject.Factory.parse(SoapUI.class.getResource("/com/eviware/soapui/resources/xsds/soapEncoding12.xsd"));
  }
 catch (  Exception e) {
    SoapUI.logError(e);
  }
 finally {
    state.restore();
  }
}
