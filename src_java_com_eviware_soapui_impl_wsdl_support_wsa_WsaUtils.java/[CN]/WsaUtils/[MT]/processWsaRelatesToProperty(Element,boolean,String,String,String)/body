{
  boolean existsWsa=getWsaProperty(header,elementLocalName) != null ? true : false;
  if (override) {
    if (existsWsa) {
      header=removeWsaProperty(override,header,elementLocalName);
    }
    header.appendChild(builder.createRelatesToElement("wsa:RelatesTo",envelopeElement,relationshipType,relatesTo));
  }
 else   if (!existsWsa) {
    header.appendChild(builder.createRelatesToElement("wsa:RelatesTo",envelopeElement,relationshipType,relatesTo));
  }
  return header;
}
