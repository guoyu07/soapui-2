{
  try {
    if (options == null) {
      options=new XmlOptions();
    }
    if (monitor != null)     monitor.setProgress(progressIndex,"Loading [" + url + "]");
    options.setLoadLineNumbers();
    return XmlObject.Factory.parse(load(url),options);
  }
 catch (  Exception e) {
    log.error("Failed to load url [" + url + "]");
    throw e;
  }
}
