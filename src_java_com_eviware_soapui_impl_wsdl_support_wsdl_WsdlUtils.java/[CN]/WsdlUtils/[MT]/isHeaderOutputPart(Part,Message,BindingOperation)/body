{
  List<SOAPHeader> headers=WsdlUtils.getExtensiblityElements(bindingOperation.getBindingOutput().getExtensibilityElements(),SOAPHeader.class);
  if (headers == null || headers.isEmpty())   return false;
  for (  SOAPHeader header : headers) {
    if (message.getQName().equals(header.getMessage()) && part.getName().equals(header.getPart()))     return true;
  }
  return false;
}
