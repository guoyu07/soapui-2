{
  super.release();
  for (  WsdlTestStep testStep : testSteps)   testStep.release();
  for (  WsdlLoadTest loadTest : loadTests)   loadTest.release();
  testRunListeners.clear();
  if (setupScriptEngine != null)   setupScriptEngine.release();
  if (tearDownScriptEngine != null)   tearDownScriptEngine.release();
}
