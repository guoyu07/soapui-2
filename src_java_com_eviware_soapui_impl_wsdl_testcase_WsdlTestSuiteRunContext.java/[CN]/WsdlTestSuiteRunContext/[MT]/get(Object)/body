{
  if ("currentTestCase".equals(key))   return getCurrentTestCase();
  if ("currentTestCaseIndex".equals(key))   return getCurrentTestCaseIndex();
  if ("settings".equals(key))   return getSettings();
  if ("testSuite".equals(key))   return getTestSuite();
  if ("testRunner".equals(key))   return getTestRunner();
  return super.get(key);
}
