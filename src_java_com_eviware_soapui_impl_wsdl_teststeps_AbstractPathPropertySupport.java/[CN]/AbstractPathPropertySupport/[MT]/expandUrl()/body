{
  String result=expand();
  try {
    if (PathUtils.isFilePath(result) && !result.startsWith("file:")) {
      result=new File(result).toURI().toURL().toString();
    }
 else {
      result=new URL(result).toString();
    }
  }
 catch (  MalformedURLException e) {
    SoapUI.logError(e);
  }
  return result;
}
