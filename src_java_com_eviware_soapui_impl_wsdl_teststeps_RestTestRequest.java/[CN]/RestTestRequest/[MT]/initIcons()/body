{
  boolean isRest=getTestStep() instanceof RestTestRequestStep;
  validRequestIcon=!isRest ? UISupport.createImageIcon("/valid_http_request.gif") : UISupport.createImageIcon("/valid_rest_request.gif");
  failedRequestIcon=!isRest ? UISupport.createImageIcon("/invalid_http_request.gif") : UISupport.createImageIcon("/invalid_rest_request.gif");
  unknownRequestIcon=!isRest ? UISupport.createImageIcon("/unknown_http_request.gif") : UISupport.createImageIcon("/unknown_rest_request.gif");
  disabledRequestIcon=!isRest ? UISupport.createImageIcon("/disabled_http_request.gif") : UISupport.createImageIcon("/disabled_rest_request.gif");
  setIconAnimator(new TestRequestIconAnimator(this));
}
