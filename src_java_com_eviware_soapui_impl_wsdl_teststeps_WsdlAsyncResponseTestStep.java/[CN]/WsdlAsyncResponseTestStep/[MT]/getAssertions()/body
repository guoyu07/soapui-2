{
  HashMap<String,TestAssertion> hashmap=new HashMap<String,TestAssertion>();
  TestAssertion testassertion;
  for (Iterator<?> iterator=getAssertionList().iterator(); iterator.hasNext(); hashmap.put(testassertion.getName(),testassertion))   testassertion=(TestAssertion)iterator.next();
  return hashmap;
}
