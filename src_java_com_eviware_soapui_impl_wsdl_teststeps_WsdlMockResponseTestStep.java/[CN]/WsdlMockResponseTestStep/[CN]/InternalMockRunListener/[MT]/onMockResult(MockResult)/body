{
  if (this.result == null && waiting && result.getMockResponse() == testMockResponse) {
    this.setResult((WsdlMockResult)result);
    testMockResponse.setMockResult(null);
    waiting=false;
    System.out.println("Got mockrequest to [" + getName() + "]");
synchronized (this) {
      notifyAll();
    }
  }
}
