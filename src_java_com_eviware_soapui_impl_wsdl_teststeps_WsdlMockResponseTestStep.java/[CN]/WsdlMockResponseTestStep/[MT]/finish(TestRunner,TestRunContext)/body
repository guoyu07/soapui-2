{
  if (listener != null) {
    mockService.removeMockRunListener(listener);
    listener=null;
  }
  if (testMockResponse != null) {
    if (testMockResponse != mockResponse) {
      mockOperation.removeMockResponse(testMockResponse);
    }
    testMockResponse=null;
  }
  if (mockRunner != null) {
    if (mockRunner.isRunning()) {
      mockRunner.stop();
    }
    mockRunner=null;
  }
}
