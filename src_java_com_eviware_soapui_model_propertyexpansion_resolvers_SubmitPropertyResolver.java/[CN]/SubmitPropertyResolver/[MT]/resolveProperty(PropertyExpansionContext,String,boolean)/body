{
  if (propertyName.charAt(0) == PropertyExpansion.SCOPE_PREFIX && context.getModelItem() instanceof AbstractHttpRequest) {
    return ResolverUtils.checkForExplicitReference(propertyName,PropertyExpansion.PROJECT_REFERENCE,((WsdlRequest)context.getModelItem()).getOperation().getInterface().getProject(),context,globalOverride);
  }
  return null;
}
