{
  for (  TestRunner testRunner : runningTestCases) {
    if (testRunner.getTestCase().getTestSuite().getProject() == project)     return true;
  }
  for (  LoadTestRunner loadTestRunner : runningLoadTests) {
    if (loadTestRunner.getLoadTest().getTestCase().getTestSuite().getProject() == project)     return true;
  }
  return false;
}
