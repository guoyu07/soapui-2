{
  if (securityTestListeners == null || securityTestListeners.length == 0)   return;
  for (int i=0; i < securityTestListeners.length; i++) {
    try {
      securityTestListeners[i].beforeRun(this,getRunContext());
    }
 catch (    Throwable t) {
      SoapUI.logError(t);
    }
  }
  super.notifyBeforeRun();
}
