{
  if (stopped)   return;
  if (status == Status.RUNNING)   status=Status.FINISHED;
  securityTest.getSecurityTestLog().addEntry(new SecurityTestLogMessageEntry("SecurityTest ended at " + new Date(System.currentTimeMillis())));
  try {
    tearDown();
  }
 catch (  Throwable e) {
    SoapUI.logError(e);
  }
  context.clear();
  stopped=true;
}
