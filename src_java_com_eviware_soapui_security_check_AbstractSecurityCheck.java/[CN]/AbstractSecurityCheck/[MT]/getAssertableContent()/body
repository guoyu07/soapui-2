{
  if (testStep instanceof WsdlTestRequestStep)   return ((WsdlTestRequestStep)testStep).getAssertableContent();
 else   if (testStep instanceof HttpTestRequest)   return ((HttpTestRequest)testStep).getAssertableContent();
 else   if (testStep instanceof RestTestRequest)   return ((RestTestRequest)testStep).getAssertableContent();
  return null;
}
