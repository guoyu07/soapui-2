{
  if (testStep instanceof WsdlTestRequest)   return ((WsdlTestRequest)testStep).getDefaultAssertableContent();
 else   if (testStep instanceof HttpTestRequest)   return ((HttpTestRequest)testStep).getDefaultAssertableContent();
 else   if (testStep instanceof RestTestRequest)   return ((RestTestRequest)testStep).getDefaultAssertableContent();
  return null;
}
