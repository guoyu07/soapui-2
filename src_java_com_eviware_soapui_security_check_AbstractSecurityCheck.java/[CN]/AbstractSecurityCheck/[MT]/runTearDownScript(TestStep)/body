{
  getScriptEngine().setScript(tearDownScript);
  getScriptEngine().setVariable("testStep",testStep);
  getScriptEngine().setVariable("log",SoapUI.ensureGroovyLog());
  try {
    getScriptEngine().run();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    getScriptEngine().clearVariables();
  }
}
