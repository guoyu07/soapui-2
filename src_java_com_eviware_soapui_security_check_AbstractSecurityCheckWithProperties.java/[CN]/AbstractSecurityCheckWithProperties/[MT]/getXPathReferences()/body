{
  List<XPathReference> result=new ArrayList<XPathReference>();
  for (  SecurityCheckedParameter param : getParameterHolder().getParameterList()) {
    TestStep t=getTestStep();
    if (t instanceof WsdlTestRequestStep) {
      if (param != null)       result.add(new XPathReferenceImpl("SecurityScan Parameter " + param.getLabel() + " in \""+ getTestStep().getName()+ "\"",((WsdlTestRequestStep)t).getOperation(),true,param,"xpath"));
    }
  }
  return result.toArray(new XPathReference[result.size()]);
}
