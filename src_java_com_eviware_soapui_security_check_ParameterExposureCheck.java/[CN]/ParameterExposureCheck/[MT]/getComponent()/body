{
  if (panel == null) {
    panel=new JPanel(new BorderLayout());
    form=new SimpleForm();
    form.addSpace(5);
    form.setDefaultTextFieldColumns(50);
    minimumCharactersTextField=form.appendTextField("Minimum Characters:","Script to use");
    minimumCharactersTextField.setText("" + ((ParameterExposureCheckConfig)config.getConfig()).getMinimumLength());
  }
  return panel;
}
