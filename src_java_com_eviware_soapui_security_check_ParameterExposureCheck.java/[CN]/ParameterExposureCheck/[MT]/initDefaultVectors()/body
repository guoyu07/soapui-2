{
  try {
    FileInputStream fstream=new FileInputStream(new File(SoapUI.class.getResource("/com/eviware/soapui/resources/security/XSS-vectors.txt").toURI()));
    DataInputStream in=new DataInputStream(fstream);
    BufferedReader br=new BufferedReader(new InputStreamReader(in));
    String strLine;
    while ((strLine=br.readLine()) != null) {
      defaultParameterExposureStrings.add(strLine);
    }
    in.close();
  }
 catch (  Exception e) {
    SoapUI.logError(e);
  }
}
