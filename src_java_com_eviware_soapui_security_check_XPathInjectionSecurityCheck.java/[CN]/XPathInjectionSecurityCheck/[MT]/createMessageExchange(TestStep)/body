{
  MessageExchange messageExchange=null;
  if (messageExchange instanceof WsdlTestRequestStep)   messageExchange=new WsdlResponseMessageExchange(((WsdlTestRequestStep)testStep).getTestRequest());
 else   messageExchange=new RestResponseMessageExchange(((RestTestRequestStep)testStep).getTestRequest());
  getSecurityCheckRequestResult().setMessageExchange(messageExchange);
}
