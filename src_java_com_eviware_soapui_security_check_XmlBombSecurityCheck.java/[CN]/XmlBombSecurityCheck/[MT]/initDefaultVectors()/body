    String strLine;
    StringBuffer value=new StringBuffer();
    while ((strLine=br.readLine()) != null) {
      value.append(strLine).append('\n');
    }
    in.close();
    XmlString bomb=xmlBombConfig.addNewXmlBombs();
    bomb.setStringValue(value.toString());
  }
