{
  String name=UISupport.prompt("Specify name for this security check","Rename SecurityCheck",target.getName());
  if (name == null || name.equals(target.getName()))   return;
  while (target.getSecurable().getSecurityCheckByName(name.trim()) != null) {
    name=UISupport.prompt("Specify unique name of SecurityCheck","Rename SecurityCheck",target.getName());
    if (name == null || name.equals(target.getName()))     return;
  }
  target.setName(name);
}
