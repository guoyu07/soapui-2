{
  SecurityCheck securityCheck=getCurrentSecurityCheck();
  String oldName=securityCheck.getName();
  String newName=UISupport.prompt("Specify new name for security check","Rename SecurityCheck",securityCheck.getName());
  while (!(newName.equals(oldName)) && (monitorSecurityTest.getSecurityCheckByName(newName) != null || monitorSecurityTest.getSecurityCheckByName(newName + " (disabled)") != null)) {
    newName=UISupport.prompt("Specify unique name for check","Rename SecurityCheck",newName + " " + (monitorSecurityTest.getMonitorSecurityChecksList().size()));
    if (newName == null) {
      return;
    }
  }
  if (newName != null && !securityCheck.getName().equals(newName)) {
    securityCheck.setName(newName);
    if (securityCheck.isDisabled()) {
      newName+=" (disabled)";
    }
    listModel.setElementAt(newName,securityChecksList.getSelectedIndex());
  }
}
