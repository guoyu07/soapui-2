{
  if (actionList == null) {
    actionList=new DefaultActionList(getSecurityCheck().getName());
    actionList.setDefaultAction(new AbstractAction(){
      public void actionPerformed(      ActionEvent e){
        UISupport.showInfoMessage("Scan [" + getSecurityCheck().getName() + "] ran with status ["+ getExecutionProgressStatus()+ "]","SecurityScan Result");
      }
    }
);
  }
  return actionList;
}
