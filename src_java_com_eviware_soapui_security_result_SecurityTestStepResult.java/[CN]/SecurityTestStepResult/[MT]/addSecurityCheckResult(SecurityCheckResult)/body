{
  if (securityCheckResultList != null)   securityCheckResultList.add(securityCheckResult);
  timeTaken+=securityCheckResult.getTimeTaken();
  if (!hasAddedRequests) {
    status=securityCheckResult.getStatus();
  }
 else   if ((securityCheckResult.getStatus() == ResultStatus.OK || securityCheckResult.getStatus() == ResultStatus.CANCELED) && status != ResultStatus.FAILED) {
    status=securityCheckResult.getStatus();
  }
 else {
    status=securityCheckResult.getStatus();
  }
  this.testLog.append(securityCheckResult.getSecurityTestLog());
  hasAddedRequests=true;
}
