{
  AttachmentElement element=holder.getList().get(rowIndex);
  if (element != null) {
switch (columnIndex) {
case 0:
      return element.isCached() ? element.getName() : element.getPath();
case 1:
    return element.getSize();
case 2:
  return element.getContentType();
case 3:
return element.isEnabled();
}
}
return null;
}
