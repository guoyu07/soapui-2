{
  if (runContext.getCurrentStep().getId().equals(testStep.getId())) {
    if (!(progressBar.getString().equals(STATE_CANCEL) || progressBar.getString().equals(STATE_MISSING_ASSERTIONS) || progressBar.getString().equals(STATE_MISSING_PARAMETERS)))     progressBar.setString(STATE_DONE);
  }
  progressBar.setBackground(new Color(240,240,240));
}
