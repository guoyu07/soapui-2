{
  if (securityCheck.getTestStep().getId().equals(testStep.getId())) {
    if (securityCheck.getSecurityCheckResult() != null && (securityCheck.getSecurityCheckResult().getStatus() != ResultStatus.CANCELED_OK || securityCheck.getSecurityCheckResult().getStatus() != ResultStatus.CANCELED_FAILED)) {
      if (progressBar.getString().equals("")) {
        progressBar.setString(STATE_RUN);
        progressBar.setForeground(OK_COLOR);
      }
      if (securityCheck.getAssertionCount() == 0) {
        if (!progressBar.getForeground().equals(FAILED_COLOR))         progressBar.setForeground(MISSING_ASSERTION_COLOR);
        progressBar.setString(STATE_MISSING_ASSERTIONS);
      }
      if (securityCheck instanceof AbstractSecurityCheckWithProperties && ((AbstractSecurityCheckWithProperties)securityCheck).getParameterHolder().getParameterList().size() == 0) {
        if (!progressBar.getForeground().equals(FAILED_COLOR))         progressBar.setForeground(MISSING_ASSERTION_COLOR);
        progressBar.setString(STATE_MISSING_PARAMETERS);
      }
    }
  }
}
