{
  return new AbstractAction(){
    public void actionPerformed(    ActionEvent e){
      MockSecurityTestRunner securityTestRunner=new MockSecurityTestRunner((SecurityTest)((SecurityCheck)SetupScriptGroovyEditorModel.this.getModelItem()).getParent());
      try {
        ((SecurityCheck)SetupScriptGroovyEditorModel.this.getModelItem()).runSetupScript(securityTestRunner,(SecurityTestRunContext)securityTestRunner.getRunContext());
      }
 catch (      Exception e1) {
        UISupport.showErrorMessage(e1);
      }
    }
  }
;
}
