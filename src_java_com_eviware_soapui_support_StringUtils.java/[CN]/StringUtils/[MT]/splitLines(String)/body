{
  try {
    ArrayList<String> list=new ArrayList<String>();
    LineNumberReader reader=new LineNumberReader(new StringReader(string));
    String s;
    while ((s=reader.readLine()) != null) {
      list.add(s);
    }
    return list;
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
