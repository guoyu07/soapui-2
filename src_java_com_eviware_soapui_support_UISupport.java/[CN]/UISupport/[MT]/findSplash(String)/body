{
  File file=new File(filename);
  URL url=null;
  try {
    if (!file.exists())     url=SoapUI.class.getResource("/com/eviware/soapui/resources/images/" + filename);
 else     url=file.toURI().toURL();
  }
 catch (  Exception e1) {
  }
  try {
    if (url == null)     url=new URL("http://www.soapui.org/images/" + filename);
  }
 catch (  Exception e2) {
    SoapUI.logError(e2);
  }
  return url;
}
