{
  if (disabled)   return true;
  String disable=System.getProperty("soapui.jxbrowser.disable","nope");
  if (disable.equals("true"))   return true;
  if (!disable.equals("false") && (!PlatformContext.isMacOS() && "64".equals(System.getProperty("sun.arch.data.model"))))   return true;
  return false;
}
