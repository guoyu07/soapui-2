{
  try {
    System.setProperty("http.proxyHost",SoapUI.getSettings().getString(ProxySettings.HOST,System.getProperty("http.proxyHost",null)));
    System.setProperty("http.proxyPort",SoapUI.getSettings().getString(ProxySettings.PORT,System.getProperty("http.proxyPort",null)));
    editorPane.setPage(new URL(url));
    return true;
  }
 catch (  NoRouteToHostException e) {
    SoapUI.log.warn("Failed to access [" + url + "]");
  }
catch (  Exception e) {
    SoapUI.logError(e);
  }
  return false;
}
