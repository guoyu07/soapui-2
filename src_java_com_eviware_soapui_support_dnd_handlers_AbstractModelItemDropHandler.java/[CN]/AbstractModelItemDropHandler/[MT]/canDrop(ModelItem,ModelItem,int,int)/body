{
  try {
    if (sourceClass.isAssignableFrom(source.getClass()) && targetClass.isAssignableFrom(target.getClass())) {
      T1 sourceItem=(T1)source;
      T2 targetItem=(T2)target;
      if ((dropAction & DnDConstants.ACTION_COPY) != 0)       return canCopy(sourceItem,targetItem,dropType);
      if ((dropAction & DnDConstants.ACTION_MOVE) != 0)       return canMove(sourceItem,targetItem,dropType);
    }
  }
 catch (  Exception e) {
    SoapUI.logError(e);
  }
  return false;
}
