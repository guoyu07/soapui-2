{
  if (modelItem instanceof AbstractHttpRequestInterface<?>)   return new AttachmentsInspector((AttachmentContainer)modelItem);
 else   if (modelItem instanceof WsdlMockResponse)   return new AttachmentsInspector(new MockRequestAttachmentsContainer((WsdlMockResponse)modelItem));
 else   if (modelItem instanceof MessageExchangeModelItem)   return new AttachmentsInspector(new WsdlMessageExchangeRequestAttachmentsContainer((MessageExchangeModelItem)modelItem));
  return null;
}
