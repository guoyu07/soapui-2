{
  contentType=contentType.toLowerCase();
  int ix=contentType.indexOf(';');
  if (ix > 0)   contentType=contentType.substring(0,ix);
  if (contentTypeToSuffix.containsKey(contentType))   return contentTypeToSuffix.get(contentType);
  for (  String key : suffixToContentType.keySet()) {
    if (key.length() == 3 && suffixToContentType.get(key).equals(contentType))     return key;
  }
  return "dat";
}
