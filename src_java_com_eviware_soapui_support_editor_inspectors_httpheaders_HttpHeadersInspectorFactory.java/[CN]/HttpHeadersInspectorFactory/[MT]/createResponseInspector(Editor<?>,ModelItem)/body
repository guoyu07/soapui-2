{
  if (JMSUtil.checkIfJMS(modelItem))   return null;
  if (modelItem instanceof AbstractHttpRequestInterface<?>)   return new HttpHeadersInspector(new WsdlRequestResponseHeadersModel((AbstractHttpRequest<?>)modelItem));
 else   if (modelItem instanceof WsdlMockResponse)   return new HttpHeadersInspector(new WsdlMockResponseHeadersModel((WsdlMockResponse)modelItem));
 else   if (modelItem instanceof MessageExchangeModelItem)   return new HttpHeadersInspector(new WsdlMessageExchangeResponseHeadersModel((MessageExchangeModelItem)modelItem));
  return null;
}
