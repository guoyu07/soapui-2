{
  if (modelItem instanceof WsdlRequest) {
    if (JMSUtil.checkIfJMS(modelItem))     return new JMSHeaderAndPropertyInspector((JMSHeaderAndPropertyInspectorModel)new ResponseJMSHeaderAndPropertiesModel((WsdlRequest)modelItem));
  }
 else   if (modelItem instanceof MessageExchangeModelItem) {
    if (JMSUtil.checkIfJMS(modelItem))     return new JMSHeaderAndPropertyInspector((JMSHeaderAndPropertyInspectorModel)new MessageExchangeResponseJMSHeaderAndPropertiesModel((MessageExchangeModelItem)modelItem));
  }
  return null;
}
