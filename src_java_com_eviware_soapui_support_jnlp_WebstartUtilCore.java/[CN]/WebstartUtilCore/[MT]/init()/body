{
  if (isWebStart()) {
    try {
      System.setProperty("wsi.dir",createWebStartDirectory("wsi-test-tools",System.getProperty("wsitesttools.jar.url")) + File.separator + "wsi-test-tools");
      System.out.println(System.getProperty("wsi.dir"));
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
