{
  String exp=expansion.toString();
  StringBuffer buf=new StringBuffer();
  for (int c=0; c < exp.length(); c++) {
    char ch=exp.charAt(c);
switch (ch) {
case '\'':
case '\\':
      buf.append('\\');
default :
    buf.append(ch);
}
}
return "var " + name + " = context.expand( \""+ buf.toString()+ "\" );\n";
}
