{
  int pos=getCaretPosition();
  String text=getText();
  while (pos < text.length()) {
    if (text.charAt(pos) == '>' && pos < text.length() - 1 && (pos > 2 && !text.substring(pos - 2,pos).equals("--")) && (pos > 1 && text.charAt(pos - 1) != '/') && text.indexOf('/',pos) == text.indexOf('<',pos) + 1 && text.lastIndexOf('/',pos) != text.lastIndexOf('<',pos) + 1) {
      setCaretPosition(pos + 1);
      return;
    }
    pos++;
  }
  getToolkit().beep();
}
