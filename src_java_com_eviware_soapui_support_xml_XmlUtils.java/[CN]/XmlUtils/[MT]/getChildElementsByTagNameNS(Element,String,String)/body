{
  List<Element> list=new ArrayList<Element>();
  NodeList nl=elm.getChildNodes();
  for (int c=0; c < nl.getLength(); c++) {
    Node item=nl.item(c);
    if (item.getParentNode() == elm && item.getNodeType() == Node.ELEMENT_NODE && localName.equals(item.getLocalName()) && namespaceUri.equals(item.getNamespaceURI()))     list.add((Element)item);
  }
  return new ElementNodeList(list);
}
