{
  try {
    XmlObject xmlObject=XmlObject.Factory.parse(xmlText,new XmlOptions());
    String namespaces=declareXPathNamespaces(xmlObject);
    if (namespaces != null && namespaces.trim().length() > 0)     xpath=namespaces + xpath;
    XmlObject[] path=xmlObject.selectPath(xpath);
    for (    XmlObject xml : path) {
      setNodeValue(xml.getDomNode(),value);
    }
    return xmlObject.toString();
  }
 catch (  Exception e) {
    SoapUI.logError(e);
  }
  return xmlText;
}
