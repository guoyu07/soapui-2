{
  super(title);
  this.editArea=editArea;
  putValue(Action.SHORT_DESCRIPTION,"Moves the caret to the specified line");
  if (UISupport.isMac()) {
    putValue(Action.ACCELERATOR_KEY,UISupport.getKeyStroke("control meta L"));
  }
 else {
    putValue(Action.ACCELERATOR_KEY,UISupport.getKeyStroke("control meta L"));
  }
}
