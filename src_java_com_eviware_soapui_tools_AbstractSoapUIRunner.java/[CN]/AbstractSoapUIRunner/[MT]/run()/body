{
  if (SoapUI.getSoapUICore() == null) {
    SoapUI.setSoapUICore(createSoapUICore(),true);
    SoapUI.initGCTimer();
  }
  setGlobalProperties(getRunnerGlobalProperties());
  SoapUIClassLoaderState state=SoapUIExtensionClassLoader.ensure();
  try {
    return runRunner();
  }
  finally {
    state.restore();
  }
}
