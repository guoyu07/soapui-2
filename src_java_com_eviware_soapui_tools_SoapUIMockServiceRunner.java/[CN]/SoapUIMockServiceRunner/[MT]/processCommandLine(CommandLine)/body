{
  if (cmd.hasOption("m"))   setMockService(getCommandLineOptionSubstSpace(cmd,"m"));
  if (cmd.hasOption("a"))   setPath(getCommandLineOptionSubstSpace(cmd,"a"));
  if (cmd.hasOption("p"))   setPort(cmd.getOptionValue("p"));
  if (cmd.hasOption("s"))   setSettingsFile(getCommandLineOptionSubstSpace(cmd,"s"));
  setBlock(!cmd.hasOption('b'));
  if (cmd.hasOption("x")) {
    setProjectPassword(cmd.getOptionValue("x"));
  }
  if (cmd.hasOption("v")) {
    setSoapUISettingsPassword(cmd.getOptionValue("v"));
  }
  return true;
}
