{
  try {
    if (path != null)     mockService.setPath(path);
    if (port != null)     mockService.setPort(Integer.parseInt(port));
    mockService.addMockRunListener(new LogListener());
    WsdlMockRunner runner=mockService.start();
    runner.setLogEnabled(false);
    runners.add(runner);
  }
 catch (  Exception e) {
    SoapUI.logError(e);
  }
}
