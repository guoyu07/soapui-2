{
  String selectedItem=getValue();
  DefaultComboBoxModel model=new DefaultComboBoxModel(values);
  if (values.length > 0 && values[0] == null) {
    model.removeElementAt(0);
    getComponent().setEditable(true);
  }
 else {
    getComponent().setEditable(false);
  }
  getComponent().setModel(model);
  if (selectedItem != null)   getComponent().setSelectedItem(selectedItem);
 else   if (getComponent().isEditable())   getComponent().setSelectedItem("");
}
