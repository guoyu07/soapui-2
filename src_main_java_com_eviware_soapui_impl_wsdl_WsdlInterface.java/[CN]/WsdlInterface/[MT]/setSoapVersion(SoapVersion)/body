{
  if (version == SoapVersion.Soap11)   getConfig().setSoapVersion(SoapVersionTypesConfig.X_1_1);
 else   if (version == SoapVersion.Soap12)   getConfig().setSoapVersion(SoapVersionTypesConfig.X_1_2);
 else   throw new RuntimeException("Unknown soapVersion [" + version + "], must be 1.1 or 1.2");
}
