{
  debuggingMode=debug;
  if (debug) {
    if (debuggingExpandedProperties.get(testCaseId) == null) {
      debuggingExpandedProperties.put(testCaseId,new StringToStringMap());
    }
  }
 else {
    if (debuggingExpandedProperties.get(testCaseId) != null) {
      debuggingExpandedProperties.remove(testCaseId);
    }
  }
}
