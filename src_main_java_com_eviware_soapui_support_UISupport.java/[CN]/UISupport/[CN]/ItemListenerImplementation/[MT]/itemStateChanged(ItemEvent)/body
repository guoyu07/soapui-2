{
  Object item=combo.getSelectedItem();
  if (item == null) {
    combo.setToolTipText(defaultTooltip);
  }
 else {
    String selectedItem=item.toString();
    if (item instanceof ModelItem)     selectedItem=((ModelItem)item).getName();
 else     if (item instanceof TestProperty)     selectedItem=((TestProperty)item).getName();
    combo.setToolTipText(selectedItem);
  }
}
