{
  String newStr="<soapenv:Envelope xmlns:soapenv=\"http://schemas.xmlsoap.org/soap/envelope/\">" + "<soapenv:Header><test/></soapenv:Header><soapenv:Body/></soapenv:Envelope>";
  String oldStr="<soapenv:Envelope xmlns:soapenv=\"http://schemas.xmlsoap.org/soap/envelope/\">" + "<soapenv:Header><test/></soapenv:Header><soapenv:Body/></soapenv:Envelope>";
  assertEquals("<soapenv:Envelope xmlns:soapenv=\"http://schemas.xmlsoap.org/soap/envelope/\">" + "<soapenv:Header><test/><test/></soapenv:Header><soapenv:Body/></soapenv:Envelope>",SoapUtils.transferSoapHeaders(oldStr,newStr,SoapVersion.Soap11));
}
