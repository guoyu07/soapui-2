{
  assertEquals("helloThere",StringUtils.createXmlName("hello there"));
  assertEquals("helloThere",StringUtils.createXmlName("hello ?? there"));
  assertEquals("hello_there",StringUtils.createXmlName("hello_there"));
  assertEquals("helloThere",StringUtils.createXmlName("hello:there"));
  assertEquals("tb_table.column",StringUtils.createXmlName("tb_table.column"));
}
